	map(0,0,0,0,16,16)
	spr(sprite,75,112)
	print("insufficient a",30,30,0)
	rectfill(0,recty0,
	120,recty1,12)
	
	if scenect >120 then
	 update = update_game
	 draw = draw_game
	end
	

end
