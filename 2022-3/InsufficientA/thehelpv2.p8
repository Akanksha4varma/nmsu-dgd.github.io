--player update functions

function plr_update()
	
	plr.dy+=grav
	plr.dx*=frict
	
	--cont
	
	if btn(⬅️) then
		plr.dx-=plr.acc
		plr.runn = true
		plr.flp = true
	end
	if btn(➡️) then
		plr.dx+=plr.acc
		plr.runn=true
		plr.flp=false
	end
	
	if plr.runn
	and not btn(⬅️)
	and not btn(➡️)
	and not plr.fall
	and not plr.jmp then
		plr.runn=false
	end
	
	--jump
	if btnp(❎)
	and plr.land then
		plr.dy-=plr.bst
		plr.land=false
	end
	
	--check collide up,down
	if plr.dy>0 then
		plr.fall=true
		plr.land=false
		plr.jmp=false
		
		plr.dy=limit_spd(plr.dy,plr.max_dy)
		
		if collide_map(plr,"down",0) then
			plr.land=true
			plr.fall=false
			plr.dy=0
			plr.y-=((plr.y+plr.h+1)%8)-1
			
			-----test-----
			collide_d="yes"
			
			else collide_d="no"
			--------------------- 
		end
			
	elseif plr.dy<0 then
		plr.jmp = true
		if collide_map(plr,"up",1) then
			plr.dy=0
			-----test-----
			collide_u="yes"
			
			else collide_u="no"
			--------------------- 
		end
	end
		
	--check left and right
	if plr.dx<0 then
		plr.dx=limit_spd(plr.dx,plr.max_dx)
		if collide_map(plr,"left",1) then
			plr.dx=0
			
			-----test-----
			collide_l="yes"
			
			else collide_l="no"
			--------------------- 
		end
	
	elseif plr.dx>0 then
	plr.dx=limit_spd(plr.dx,plr.max_dx)
		if collide_map(plr,"right",1) then
			plr.dx=0
			
			-----test-----
			collide_r="yes"
			
			else collide_r="no"
			--------------------- 
		end
	end
	plr.x+=plr.dx
	plr.y+=plr.dy
	
	--limit player ot map
	if plr.x < map_start then
		plr.x=map_start
	end
	
	if plr.x>map_end-plr.w then
		plr.x=map_end - plr.w
	end
	
end

function plr_anim()
	
	if plr.jmp then
		plr.sp=6
	elseif plr.fall then
		plr.sp=7
	elseif plr.runn then
		if time()-plr.anim>.1 then
			plr.anim=time()
			plr.sp+=1
			if plr.sp>5 then
				plr.sp=2
			end
		end
	else --player idle
		if time()-plr.anim>.3 then
			plr.anim=time()
			plr.sp+=1
			if plr.sp>2then
				plr.sp=1
			end
		end
	end
end

function limit_spd(num,maximum)
	return mid(-maximum,num,maximum)
end
