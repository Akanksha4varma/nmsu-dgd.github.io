--var
function _init()
	plr = {
		sp = 1,
		x = 59,
		y = 59,
		w = 8,
		h = 8,
		flp = false,
		dx = 0,
		dy = 0,
		max_dx =2.5,
		max_dy =3,
		acc= 0.60,
		bst = 4,
		anim = 0,
		fall = false,
		runn = false,
		jmp = false,
		land = false,
		
	}
	grav = 0.32
	frict= 0.8
	
	--simple camera 
	cam_x=0
	
	--limit the map
	map_start=0
	map_end=1024
	
	-------------test------
	x1r=0 y1r=0 x2r=0 y2r=0
	collide_l="no"
	collide_r="no"
	collide_d="no"
	collide_u="no" 
	-----------------------
	
	_update = update_logo
	_draw = draw_logo
end



function update_menu()
	if btnp(❎) then
		--start 
		_update = update_game
		_draw = draw_game
	end
end

function draw_menu()
	cls()
	print("the help",30,30)
	print("press ❎ to start",30,60)
end

