--var

function _init()
	--All the objects of the player. 
	plr = {
		sp = 1,
		x = 59,
		y = 190,
		w = 8,
		h = 8,
		dead = false,
		flp = false,
		dx = 0,
		dy = 0,
		max_dx =2.5,
		max_dy =3,
		acc= 0.60,
		bst = 4,
		anim = 0,
		fall = false,
		runn = false,
		jmp = false,
		land = false,
		
	}
	--This will hold all current enemies and 
	--can create more 
	enemies = {
		make_enemy(16,59,90),
		make_enemy(32,260,136),
		make_enemy(48,980,90),
	}
	--Basic movement options
	grav  = 0.32
	frict = 0.8
	level = 1
	
	--simple camera 
	cam_x=0
	cam_y=0
	
	--limit the map
	map_start = 0
	map_end   = 1024
	
	-------------test------
	x1r=0 y1r=0 x2r=0 y2r=0
	collide_l="no"
	collide_r="no"
	collide_d="no"
	collide_u="no" 
	-----------------------
	
	--Send to draw thelogo and update immediately
	_update = update_logo
	_draw = draw_logo
end


--update menu
function update_menu()
	if btnp(❎) then
		--start the game by changing the
		--update calls
		_update = update_game
		_draw = draw_game
	end
end

--draw it 
function draw_menu()
	cls()
	print("the help",30,30)
	print("press ❎ to start",30,60)
end


--update menu
function update_dead()
	if btnp(❎) then
		--Death menu and call back to game
		--once we decide to restart
		plr.dead = false
		_update = update_game
		_draw = draw_game
	end
end

--draw it 
function draw_dead()
	cls()
	print("you died",plr.x-30,plr.y-26)
	print("press ❎ to restart",plr.x-30,plr.y-20)
end



--enemy update func

function make_enemy(sprite,x,y)
	--Enemy will create behaviors for the enemies
	--They will all share basic same behaviors 
	local enemy={
		x = x,
		y = y,
		anim = 0,
		ogsp = sprite,
		sp = sprite,
		is_dead = false,
		
		update = function(self)
			
			--follow player based on difference in x and y
			local xdiff = self.x - plr.x
			local ydiff = plr.y - self.y
			
			--Animate the sprite
			if(time() - self.anim > .2) then
				self.anim = time()
				self.sp +=1
			end
				
			if(self.sp > self.ogsp + 1)then
				self.sp = self.ogsp
			end
			
			if((abs(xdiff) < 45) 
						and
					(abs(ydiff) < 18)) then
				self.x -= xdiff /20
			end
			--if player is clsoe enough, kill
			if(abs(xdiff) < 3 and 
			   abs(ydiff) < 10) then
				plr.x = 59
				plr.y = 190
			   plr.dead = true
			end
		end,
		--draw the sprite
		draw = function(self)
			if not self.is_dead then
				spr(self.sp,self.x,self.y,1,1)
			end
		end
		
	}
	return enemy
end
