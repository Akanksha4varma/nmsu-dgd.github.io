if sprite_anim > 1 then
	sprite+=1
	sprite_anim= 0
end

if sprite > 114 then
 sprite = 112 
end


scenect+=1

end

function draw_logo()

	cls()
	map(112,48,0,0,16,16)
	spr(sprite,75,112)
	print("insufficient a",30,30,0)
	rectfill(0,recty0,
	120,recty1,12)
	
	if scenect >120 then
	 _update = update_menu
	 _draw = draw_menu
	end
	

end
