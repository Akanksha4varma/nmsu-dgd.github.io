--game code
function update_game()
	--set dead to false when game starts
	plr.dead = false
	--update player and animate them
	plr_update()
	plr_anim()
	--create all enemy objects as well
	for enemy in all(enemies) do
		enemy:update()
	end
	
	--simp cam
	cam_x=plr.x-64+(plr.w/2)
	
	
	

	 cam_y=plr.y-80+(plr.h/2)
	if cam_x < map_start then
		cam_x=map_start
	end
	if cam_x > map_end-128 then
		cam_x = map_end-128
	end
	

	camera(cam_x,min(cam_y,110))
	
	--If player dies
	if(plr.dead)then
		_update = update_dead
		_draw = draw_dead
	end
	
end

function draw_game()
	cls(1)
	--Draw the map
	if(level == 1) then
	 map(0,0)
	 spr(plr.sp,plr.x,plr.y,1,1,plr.flp)
	 local enemy
	
	for enemy in all(enemies) do
		enemy:draw()
	end
	
	end
	--This is all debug code
	//print("dead? "..(plr.dead and 'true' or 'false') ,plr.x,plr.y-10)
